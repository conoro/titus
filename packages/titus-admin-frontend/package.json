{
  "name": "titus-admin-frontend",
  "version": "1.0.0-alpha.1",
  "private": true,
  "main": "cjs/app.js",
  "module": "es/app.js",
  "files": [
    "cjs/",
    "es/",
    "src/"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "auth0-js": "^9.14.0",
    "aws-amplify": "^3.3.8",
    "cross-env": "^7.0.2",
    "env-schema": "^2.0.1",
    "fluent-schema": "^1.0.5",
    "formik": "^2.2.5",
    "graphql-hooks": "^5.0.0",
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "jwt-decode": "^3.1.2",
    "pino": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-adal": "^0.5.0",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "regenerator-runtime": "^0.13.7",
    "titus-admin-backend": "^1.0.0-alpha.1",
    "yup": "^0.29.3"
  },
  "scripts": {
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.sample', '.env', fs.constants.COPYFILE_EXCL)\"",
    "build": "react-scripts build",
    "predist": "rm -rf node_modules/react node_modules/react-dom",
    "predist:watch": "npm run predist",
    "_dist:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir cjs --copy-files",
    "_dist:es": "babel src --out-dir es --copy-files",
    "_dist_watch:commonjs": "npm run _dist:commonjs -- --watch",
    "_dist_watch:es": "npm run _dist:es -- --watch",
    "dist": "run-p _dist:*",
    "dist:watch": "run-p _dist_watch:*",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "start:admin": "npm run validate:env && cross-env PORT=3001 react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "test": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage  --colors",
    "test:local": "cross-env CI=false react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage  --colors",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:related": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen --onlyChanged",
    "validate:env": "node bin/validateEnv.js",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "babel-loader": "^8.2.1",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "puppeteer": "^5.5.0",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "serve-handler": "^6.1.3",
    "storybook-readme": "^5.0.9",
    "typescript": "^4.0.5"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*js",
      "!**/__snapshots__",
      "!src/**/*.story.js",
      "!**/serviceWorker.js",
      "!src/index.js"
    ]
  },
  "proxy": "http://localhost:5001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
